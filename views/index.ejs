    }

    .feed {
      color: white;
      background: rgba(0, 0, 0, 0.8);
    }

    .social a:hover {
      transform: translate(0px, 0);
    }

    /*footer section ends*/


    /*last button starts*/
    #myBtn {
      display: none;
      position: fixed;
      bottom: 2px;
      right: 40px;
      z-index: 99;
