  <style>
    * {
      margin: 0px;
      padding: 0px;
      box-sizing: border-box;
    }

    html {
      scroll-behavior: smooth;
    }

    body {
      overflow-x: hidden;
    }


    .circle {
      display: inline-block;
      background-color: purple;
      height: 40px;
      width: 40px;
      border-radius: 50%;
      transform: scale(0);
      animation: grow 1.5s linear infinite;
      margin: -10px;
    }

    .circle:nth-child(2) {
      background-color: palevioletred;
      animation-delay: 0.75s;
    }

    @keyframes rotate {
      to {
        transform: rotate(360deg);
      }
    }

    @keyframes grow {
      50% {
        transform: scale(1);
      }
    }

    .navbar {
      background: black;
      opacity: 0.9;
      color: white;
      padding-right: 30px;
      padding-top: 2px;
      padding-bottom: 2px;
    }

    .navbar-toggler {
      border: 2px solid white;

    }

    a {
      text-decoration: none;

    }

    a:hover {
      text-decoration: none;

    }

    li {
      list-style: none;
    }

    .header {

      width: 100%;
      height: 100vh;
      background: rgba(0, 0, 0, 0.5) url('bg17.jpeg') no-repeat center center fixed;
      background-size: cover;
      background-blend-mode: darken;
      position: relative;
      -webkit-background-size: cover;
      -moz-background-size: cover;
      -o-background-size: cover;

    }

    .header::before {
      content: "";
      position: absolute;
      opacity: 0.4;
      z-index: -1;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
      background: rgba(0, 0, 0, 0.6) url('bg17.jpeg') no-repeat center center fixed;
      background-size: cover;
      background-blend-mode: darken;
      -webkit-background-size: cover;
      -moz-background-size: cover;
      -o-background-size: cover;

    }
     .navbar-brand {
      margin-left: 15px;
      padding-top: 5px;
      font-size: 2.3rem;
      color: white !important;
      font-weight: 400 !important;
      font-family: 'Lemonada', cursive;
    }

    .navbar::before {
      content: "";
      position: absolute;
      opacity: 0.9;
      z-index: -1;
      background: rgba(0, 0, 0, 1.3) url('bg3.jpeg') no-repeat center center fixed;

    }

    .nav-item a {
      font-weight: bold;
      color: white !important;
    }

    .nav-item {
      padding: 0 23px;
    }

    nav ul li a {
      position: relative;
    }

    nav ul li a:hover {
      color: orangered !important;
      transition: .9s;

    }

    nav ul li a::after {

      content: "";
      position: absolute;
      left: 50%;
      bottom: 0;
      transform: translateX(-50%) scaleX(0);
      -webkit-tranform: translateX(-50%) scaleX(0);
      transform-origin: 50% 50%;
      width: 100%;
      height: 2px;
      background-color: orangered;
      -webkit-transition: transform 250ms;
      transition: transform 250ms;
    }

    nav ul li a:hover:after {
      -webkit-transform: translateX(-50%) scaleX(1);
      transform: translateX(-50%) scaleX(1);
    }

    .header-section {
      font-family: 'Flamenco', cursive;
      width: 100%;
      height: inherit;
      color: white;
      text-align: center;
      position: relative;
    }

    .divdiv {
      width: 14px;
      height: 14px;
      border: 2px solid orangered;
      align-items: center;

      justify-content: center;
      display: inline-flex;
      margin: 21px 3px;
    }

    .center-div {
      font-family: 'Flamenco', cursive;
      width: 100%;
      height: auto;
      position: absolute;
      top: 60%;
      left: 50%;
      transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
      -webkit-transform: translate(-50%, -50%);
    }

    .center-div p {
      font-size: 1.4rem;
      font-weight: 600;
      line-height: 1.6;
      color: white;
      opacity: 0;
      animation: fadeUp 0.5s forwards;
      animation-delay: 0.5s;
      margin-bottom: 25px !important;

    }

    .margin {
      margin: 22px 0 !important;
    }

    .center-div h1 {
      animation: scale 0.6s forwards;

    }

    .change {
      animation: colorchange 1s infinite;
      font-size: 3.5rem;
    }

    .btn {
      display: inline-block;
      padding: 6px 40px;
      font-weight: bold;
      text-decoration: none;
      border-radius: 200px;
      transition: background-color 0.2s, border 0.2s, color 0.2s;
      font-size: 1rem;
      text-align: center;
      line-height: 1.4;

    }

    .btnfull:hover {
      border: 2px solid orangered;
      background: transparent;
      color: orangered;


    }

    .btnfull {
      background-color: orangered;
      font-size: 18px;
      font-weight: bold;
      color: white;
      animation: fadeDown .5s forwards;
      opacity: 0;
      animation-delay: 1.2s;
      position: relative;
      cursor: pointer;
      overflow: hidden;
    }

   

    @keyframes colorchange {
      0% {
        color: purple
      }

      25% {
        color: orangered;
      }

      50% {
        color: white;
      }

      100% {
        color: orangered;
      }
    }

    @keyframes fadeUp {
      0% {
        transform: translateY(4rem);
      }

      100% {
        opacity: 1;
        transform: translateY(0);
      }
    }

    @keyframes scale {
      0% {
        transform: scale(2);
      }

      100% {
        opacity: 1;
        transform: scale(1);
      }
    }

    @keyframes fadeDown {
      0% {
        transform: translateY(-1rem);
      }

      100% {
        opacity: 1;
        transform: translateY(0);
      }
    }

    /*navbar end*/
    /*feedback start*/

    .leftside {
      display: flex;
      flex-direction: column-reverse;
      position: absolute;
      top: 25%;
    }

    .leftside a {
